{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Supabase MCP Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@supabase/mcp-server-supabase/dist/index.js",
      "envFile": "${workspaceFolder}/.env.local",
      "console": "integratedTerminal"
    },
    {
      "name": "Next.js Development",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
      "args": ["dev"],
      "envFile": "${workspaceFolder}/.env.local",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    }
  ]
}
